{
  "name": "amazon-scraper-v2",
  "version": "2.0.0",
  "description": "Amazon product scraper with admin dashboard",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch server/index.js",

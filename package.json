    "bull": "^4.12.0",
    "playwright": "^1.40.0",
    "https-proxy-agent": "^7.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "vite": "^5.0.8",
    "@vitejs/plugin-vue": "^4.5.2",
    "vue": "^3.3.13",
    "vue-router": "^4.2.5",
    "element-plus": "^2.4.4",
    "@element-plus/icons-vue": "^2.3.1",
    "pinia": "^2.1.7",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2"
  }
